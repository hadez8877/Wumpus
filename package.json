{
  "name": "wumpus-bot",
  "version": "0.0.1",
  "description": "ðŸª‚ Discord bot designed to provide advanced and highly customizable moderation.",
  "main": "dist/index.js",
  "scripts": {
    "watch": "tsc-watch --build --onSuccess \"node start-dev.js\"",
    "start-bot-prod": "node --enable-source-maps --stack-trace-limit=30 dist/index.js",
    "start-bot-dev": "node --enable-source-maps --stack-trace-limit=30 --inspect=0.0.0.0:9229 dist/index.js",
    "start-bot": "pnpm run start-bot-prod",
    "build": "tsc --build",
    "run-tests": "ava",
    "test-watch": "tsc-watch --build --onSuccess \"pnpx ava\"",
    "lint": "eslint --ext .ts,.js,.tsx .",
    "lint:fix": "eslint --ext .ts,.js,.tsx . --fix",
    "format": "prettier --write src",
    "format:check": "prettier --check src"
  },
  "keywords": [],
  "author": "Pedro Fabricio (@Hadez)",
  "license": "MIT",
  "dependencies": {
    "discord.js": "14.15.3",
    "dotenv": "16.4.5",
    "kleur": "4.1.5",
    "mongoose": "8.5.1",
    "readline": "1.3.0",
    "tlds": "1.254.0",
    "tmp": "0.2.3",
    "tsc-watch": "6.2.0",
    "tsconfig-paths": "4.2.0",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "9.7.0",
    "@types/node": "20.14.10",
    "@types/tmp": "0.2.6",
    "@typescript-eslint/eslint-plugin": "7.17.0",
    "@typescript-eslint/parser": "7.17.0",
    "ava": "6.1.3",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-n": "17.9.0",
    "eslint-plugin-promise": "6.6.0",
    "eslint-plugin-unused-imports": "4.0.1",
    "globals": "15.8.0",
    "prettier": "3.3.3",
    "standard": "17.1.0",
    "typescript": "5.5.3",
    "typescript-eslint": "7.16.1"
  },
  "ava": {
    "files": [
      "dist/**/*.test.js"
    ],
    "require": [
      "./register-tsconfig-paths.js"
    ]
  },
  "eslintConfig": {
    "extends": [
      "standard"
    ]
  }
}
