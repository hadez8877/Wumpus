{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": [
    "standard"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "no-console": "off",
    "no-unused-vars": "off",
    "eqeqeq": "error",
    "no-var": "off",
    "prefer-const": "off",
    "no-unreachable": "off"
  }
}